  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" async></script>
 <b:include data='blog' name='all-head-content'/>
 
<title>

 <b:if cond='data:blog.pageType == &quot;index&quot;'>
 <data:blog.pageTitle/>
 <b:else/>
 <b:if cond='data:blog.pageType != &quot;error_page&quot;'>
 <data:blog.pageName/> | <data:blog.title/>
 <b:else/>
 Page Not Found | <data:blog.title/>
 </b:if>
 </b:if>
 </title>
 <b:skin><![CDATA[

body {
text-align: center;
}
header .h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
	margin-bottom: .5rem;
	font-family: inherit;
	font-weight: 500;
	line-height: 1.2;
	color: inherit;
	text-align: center;

}
aside h2 {
background: #262424;
font-family: sans-serif;
border-bottom: 2px solid;
color: white;
padding: 2px;
}
]]></b:skin>

</head>

<body>

<header>

<b:section class='header container-fluid' id='header' maxwidgets='1' showaddelement='no'>
<b:widget id='Header1' locked='true' title='' type='Header'></b:widget>
</b:section>

</header>

<article>

<b:section class='main  container-fluid' id='main' preferred='yes' showaddelement='yes'>
<b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' class='container-fluid'>
</b:widget>
</b:section>

</article>

<aside>

<b:section class='sidebar' id='sidebar' maxwidgets='' showaddelement='yes'>
